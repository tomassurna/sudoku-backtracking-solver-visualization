{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "parser": "typescript",
  "overrides": [
    {
      "files": [
        ".{js,ts,tsx}"
      ],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": [
        ".json",
        ".jsonc",
        ".babelrc",
        ".prettierrc"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": ".css",
      "options": {
        "parser": "css"
      }
    },
    {
      "files": "*.md",
      "options": {
        "parser": "markdown"
      }
    }
  ]
}
